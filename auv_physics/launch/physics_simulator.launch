<launch>
    <arg name="pkg" value="auv_physics" />
    <arg name="name" value="physics_simulator" />
    <arg name="type" default="$(arg name)" />
    <arg name="rate" default="30" />
    <arg name="args" value="_loop_rate:=$(arg rate)" />
    
    <node
        pkg="$(arg pkg)"
        type="$(arg type)"
        name="$(arg name)"
        args="$(arg args)"
        output="screen" />

</launch>
